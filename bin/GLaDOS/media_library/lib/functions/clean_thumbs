#!/bin/bash

function clean_thumbs(){
inputFile="$1"
inputFile="${inputFile:0:$((${#inputFile}-4))}"
thumbDir=/shares/glados/wheatley

S="$2"
Q="$3"
D="$4"
T="$5"

genindex="$thumbDir/gen_index"

if [ -d "$thumbDir/$inputFile" ]; then
 echo "Deleting $inputFile thumbnails..."
 rm -rf "$thumbDir/$inputFile"
 $genindex
#else
# echo "$thumbDir/$inputFile not found"
fi
} #clean_thumbs

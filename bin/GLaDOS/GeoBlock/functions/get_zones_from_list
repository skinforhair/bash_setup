#!/bin/bash
function get_zones_from_list(){
while read s; do

 #strip everything up to and including the first instance of '('
 browser="${s#*(}"

 # strip everything after the first ')', again, inclusive
 browser="${browser%%)*}"

 # ...and show the result...
 echo "http://www.ipdeny.com/ipblocks/data/aggregated/${browser,,}-aggregated.zone"
done < input.txt
}


     ${tan} \\\\\\ ${NC}8( )\\\\  ${orange}//${NC}
     ${white} _${tan}||${NC} []88<${orange}//${NC}
${white}     / ${tan}||${NC}88== ${orange}||${NC}
${white}   [${NC}\\\\\\8${tan}||${NC}|___|${orange}//${NC}
${white}  /${NC}/8\\\\\\]${tan}||${NC}000${orange}||${NC}
${white} [${NC}] |\\\\\\8${tan}||${NC}8) ${orange}//${NC}        ${white}_________${NC}
${white} /${NC}/ /888${tan}||${NC}88${orange}//${NC}___     ${white}/\\       \\\\\\ ${NC}
${white}[${NC}] /888${red}@@@${NC}8${orange}||${NC}8888]   ${white}|| |  ___  ||${NC}
${white}[${NC}] (|${red}@@(*)@@${orange})${NC}888 []  ${white}|| | /   \ ||${NC}
${white}[${NC}] \888${red}@@@${NC}8)${white}++++${NC}  [] ${white}|| | |   | ||${NC}
